estudo=$1
estudoDTI=$estudo/DTIfsl
estudoDTIFreeSurfer=$estudo/DTIfslFreeSurfer
boolnames=0
boolDWIfiles=0
j=0
k=0
m=0

for i in $*; do
	if [$i!="-s"]; then
		if [$i!="-d"] && [$boolnames==1]; then
			NAMESubject[j]=$i
			j=j+1
		elif [$boolDWIfiles==1]; then
			DWIfiles[k]=$i
			k=k+1
		elif [$i!="-d"]
			boolnames=0
			boolDWIfiles=1
		fi
	else
		boolnames=1;	
	fi

done

for l in NAMESubject[*]; do
 	./scriptDTIfsl ${ARRAYSTRUCTURE[$m]} $estudoDTI/$j $j
	m=m+1

done

./pipeline $estudoDTIFreeSurfer $estudoDTI -l NAMESubject[@]
